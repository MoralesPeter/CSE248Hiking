   <children>
      <ListView fx:id="lvHistory" layoutX="17.0" layoutY="14.0" prefHeight="379.0" prefWidth="566.0" />
      <Button fx:id="btnViewPictures" layoutX="16.0" layoutY="401.0" mnemonicParsing="false" onAction="#openHikingPictures" prefHeight="58.0" prefWidth="568.0" text="View Pictures">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
      </Button>
      <Button fx:id="btnBack" layoutX="523.0" layoutY="468.0" mnemonicParsing="false" onAction="#openTrailView" text="Go Back" />
   </children>
</AnchorPane>
